<% if @states.present? %>
  <%= turbo_stream.replace "country_states_list" do %>
    <div class="" id="country_states_list">
      <label>States</label>
      <select name="user[state]" id="user_state">
        <% @states.each do |state| %>
          <option value="<%= state[0] %>"><%= state[1] %></option>
        <% end %>
      </select>
    </div>
  <% end %>
<% end %>